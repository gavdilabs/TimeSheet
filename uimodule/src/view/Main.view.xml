<mvc:View height="100%" controllerName="com.gavdilabs.timesheet.controller.Main"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

			<Page showHeader="false">
					<List
						headerText="Employees"
						items="{
							path: '/Employees'
						}" >
						<StandardListItem
							title="{firstName}"
							counter="{lastName}"/>
					</List>
			</Page>

</mvc:View>